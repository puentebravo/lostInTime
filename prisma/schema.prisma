generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model posts {
  ID      String  @id @unique(map: "ID") @default(uuid()) @db.Char(36)
  title   String  @db.VarChar(50)
  content String  @db.LongText
  tags    String? @db.VarChar(20)
}
