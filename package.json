{
  "name": "lostintime",
  "version": "1.0.0",
  "description": "A custom website for a travel blog",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.ts",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puentebravo/lostInTime.git"
  },
  "author": "Charlie Puente",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/puentebravo/lostInTime/issues"
  },
  "homepage": "https://github.com/puentebravo/lostInTime#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.3.0",
    "sequelize": "^6.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "@types/sequelize": "^4.28.10",
    "autoprefixer": "^9.8.6",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "typescript": "^4.4.3"
  }
}
